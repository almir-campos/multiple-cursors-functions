{
  "atom-text-editor:not([mini])": {
    "ctrl-alt-r": "multiple-cursors-functions:reverse-selections",
    "ctrl-alt-n": "multiple-cursors-functions:insert-numbers",
    "ctrl-alt-l": "multiple-cursors-functions:insert-letters",
    "ctrl-alt-0": "multiple-cursors-functions:insert-text",
    "ctrl-alt-s": "multiple-cursors-functions:split-selections",
    "ctrl-alt-c": "multiple-cursors-functions:selections-to-cursors"
  },
  "atom-text-editor[mini]": {
    "enter": "core:confirm",
    "escape": "core:cancel"
  },
  ".platform-darwin atom-text-editor[mini]": {
    "cmd-w": "core:cancel"
  },
  ".platform-win32 atom-text-editor[mini]": {
    "ctrl-w": "core:cancel"
  },
  ".platform-linux atom-text-editor[mini]": {
    "ctrl-w": "core:cancel"
  }
}
