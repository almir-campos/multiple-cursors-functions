{
  ".multiple-cursors-functions .platform-darwin, .platform-win32, .platform-linux": {
    "ctrl-alt-r": "multiple-cursors-functions:reverse-selections",
    "ctrl-alt-n": "multiple-cursors-functions:insert-numbers-simple",
    "ctrl-alt-l": "multiple-cursors-functions:insert-letters",
    "ctrl-alt-0": "multiple-cursors-functions:insert-numbers"
  },

  "atom-text-editor[mini]": {
    "enter": "core:confirm",
    "escape": "core:cancel"
  },
  ".platform-darwin atom-text-editor[mini]": {
    "cmd-w": "core:cancel"
  },
  ".platform-win32 atom-text-editor[mini]": {
    "ctrl-w": "core:cancel"
  },
  ".platform-linux atom-text-editor[mini]": {
    "ctrl-w": "core:cancel"
  }
}
